    {
      "name": "decks",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"decks\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"name\" TEXT NOT NULL, \"card_count\" INTEGER NOT NULL DEFAULT 0);"
        }
      ]
    },
    {
      "name": "flashcards",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"flashcards\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"deck_id\" INTEGER NOT NULL REFERENCES decks (id), \"front\" TEXT NOT NULL, \"back\" TEXT NOT NULL, \"next_review_date\" INTEGER NULL, \"last_review_date\" INTEGER NULL, \"state\" INTEGER NOT NULL, \"step\" INTEGER NULL, \"stability\" REAL NULL, \"difficulty\" REAL NULL);"
        }
      ]
    },
    {
      "name": "reviews",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"reviews\" (\"card_id\" INTEGER NOT NULL REFERENCES flashcards (id), \"rating\" INTEGER NOT NULL, \"review_date\" INTEGER NOT NULL, PRIMARY KEY (\"card_id\", \"review_date\"));"
        }
      ]
    },
    {
      "name": "user_progress",
      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"user_progress\" (\"id\" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, \"currency\" INTEGER NOT NULL DEFAULT 0, \"last_reset_date\" INTEGER NULL, \"login_quest_done\" INTEGER NOT NULL DEFAULT 0 CHECK (\"login_quest_done\" IN (0, 1)), \"login_quest_claimed\" INTEGER NOT NULL DEFAULT 0 CHECK (\"login_quest_claimed\" IN (0, 1)), \"learn_quest_done\" INTEGER NOT NULL DEFAULT 0 CHECK (\"learn_quest_done\" IN (0, 1)), \"learn_quest_claimed\" INTEGER NOT NULL DEFAULT 0 CHECK (\"learn_quest_claimed\" IN (0, 1)), \"cards_learned_today\" INTEGER NOT NULL DEFAULT 0, \"review_quest_done\" INTEGER NOT NULL DEFAULT 0 CHECK (\"review_quest_done\" IN (0, 1)), \"review_quest_claimed\" INTEGER NOT NULL DEFAULT 0 CHECK (\"review_quest_claimed\" IN (0, 1)), \"all_daily_quests_done\" INTEGER NOT NULL DEFAULT 0 CHECK (\"all_daily_quests_done\" IN (0, 1)), \"all_daily_quests_claimed\" INTEGER NOT NULL DEFAULT 0 CHECK (\"all_daily_quests_claimed\" IN (0, 1)), \"unlocked_green\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_green\" IN (0, 1)), \"unlocked_blue\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_blue\" IN (0, 1)), \"unlocked_yellow\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_yellow\" IN (0, 1)), \"unlocked_pink\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_pink\" IN (0, 1)), \"unlocked_orange\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_orange\" IN (0, 1)), \"unlocked_purple\" INTEGER NOT NULL DEFAULT 0 CHECK (\"unlocked_purple\" IN (0, 1)));"
